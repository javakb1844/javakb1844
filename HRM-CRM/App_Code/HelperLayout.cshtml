@helper MenuItem(System.Web.Mvc.WebViewPage wV, LeftMenuViewModel item)
{

    var temp = item.ActionName + "_" + item.ControllerName;
    var result = item.ActionName.Split('?');
    if (result.Length > 1)
    {
        var tempaction = result[1].Split('=');
        if (tempaction[0] == "employee")
        {
            <div class="menu-item">
                <a class="menu-link active" href="@wV.Url.Action(result[0], item.ControllerName, new { employee = tempaction[1] })" parent="@item.ParentId" leftid="@item.LeftMenuId" id="a_@temp">
                    <span class="menu-bullet">
                        <span class="bullet bullet-dot"></span>
                    </span>
                    <span class="menu-title">@item.MenuName</span>
                </a>
            </div>

        }
        else
        {
            <div class="menu-item">
                <a class="menu-link active" href="@wV.Url.Action(item.ActionName, item.ControllerName)" parent="@item.ParentId" leftid="@item.LeftMenuId" id="a_@temp">
                    <span class="menu-bullet">
                        <span class="bullet bullet-dot"></span>
                    </span>
                    <span class="menu-title">@item.MenuName</span>
                </a>
            </div>
        }
    }

}
@helper MenuItemNew(System.Web.Mvc.WebViewPage wV, LeftMenuViewModel item)

                                                                    {
                                                                        validationText.Add(iteminner.Key + ": {validators: {  notEmpty: {  message: '" + iteminner.Key + " is required'  }}} ");
                                                                    }
                                                                }
                                                                else
                                                                {
                                                                    @HelperLayout.InputText(iteminner.Key, iteminner.Key, iteminner.Key, temp.KeyValue, lastPart, IsReadOnly, IsCompulsory)
                                                                    if (IsCompulsory)
                                                                    {
                                                                        validationText.Add(iteminner.Key + ": {validators: {  notEmpty: {  message: '" + iteminner.Key + " is required'  }}} ");
                                                                    }
                                                                }
                                                            </div>

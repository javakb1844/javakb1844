    @helper SelectBox(string Label, string ElementId, string ElementName, string ElementValue, string Selected, string ReplaceStr, bool IsReadOnly, bool IsCompulsory)
    {

        if (IsReadOnly)
        {
            <label class="fs-6  mb-2">
                <span class="fw-bold">@Label.Replace(ReplaceStr + "_", ""):</span> <span> @ElementValue</span>
            </label>
        }
        else
        {
            string required = "";
            if (IsCompulsory)
            {
                required = "required";
            }
            <label class="fs-6 fw-bold mb-2">
                <span class="required">@Label.Replace(ReplaceStr + "_", "")</span>
            </label>
            var okh = ElementValue.Replace("[", "").Replace("]", "").Replace("}", "").Split('{');
            var testArray = okh.Where(x => x.IsNotNullOrEmpty()).ToArray();
            <select name="@ElementName" id="@ElementId" aria-label="Select a Country" data-control="select2" data-placeholder="Select a Country..." data-dropdown-parent="#kt_modal_add_customer" class="form-select form-select-solid fw-bolder" @required>
                @foreach (var itemSlect in testArray)
                {
                    var tempOPtion = itemSlect.Split(',');
                    var Tvalue = tempOPtion[0].Split(':')[1];
                    var TCap = tempOPtion[1].Split(':')[1].Replace("\"", "");
                    <option value="@Tvalue" @if (Selected == TCap) { @("selected") }>@TCap</option>
                }
            </select>
        }

    }
    @helper InputText(string Label, string ElementId, string ElementName, string ElementValue, string ReplaceStr, bool IsReadOnly, bool IsCompulsory)
    {
        if (IsReadOnly)
        {
            <label class="fs-6  mb-2">
                <span class="fw-bold">@Label.Replace(ReplaceStr + "_", ""):</span> <span> @ElementValue</span>
            </label>

        }
        else if (IsCompulsory)
        {
            <label class="fs-6 fw-bold mb-2">
                <span class="required">@Label.Replace(ReplaceStr + "_", "")</span>
            </label>
            <input type="text" class="form-control form-control-solid" placeholder="" patterns="/^[a-z\d]{5,12}$/i" name="@ElementName" id="@ElementId" value="@ElementValue" required />
            <div class="valid-feedback">
                Looks good!
            </div>
            <div class="invalid-feedback">
                Please provide a valid city.
            </div>
        }
        else
        {
            <label class="fs-6 fw-bold mb-2">
                <span>@Label.Replace(ReplaceStr + "_", "")</span>
            </label>
            <input type="text" class="form-control form-control-solid" placeholder="" name="@ElementName" id="@ElementId" value="@ElementValue" />

        }
    }

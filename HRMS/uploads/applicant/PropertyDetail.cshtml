<script language="javascript">

    function tweetCurrentPage(title) {
        var urlLink = document.location.href;
        var abc = "https://twitter.com/intent/tweet?text=" + title + "&url=" + urlLink;
        var abcL = abc.length;
        if (abcL > 182) {
            var ss = urlLink.split("_");
            var ttt = abcL - 182;
            ss[0] = ss[0].substring(0, (ss[0].length - ttt));
            var abc = "https://twitter.com/intent/tweet?text=" + title + "&url=" + ss[0] + "_" + ss[1];

        }
        window.open(abc, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600'); return false;
    }

</script>
<script language="javascript">

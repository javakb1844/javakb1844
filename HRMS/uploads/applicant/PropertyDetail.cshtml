@model VM.Property.SimilarSoldPropertiesViewModel
@{
    ViewBag.Title = "ilaan.com";
    Layout = "~/Views/Shared/header_Layout.cshtml";
 var tempMainTag = ViewBag.Meta;
    var fooArray = tempMainTag.Split('<');
    var Ftag = "";

    foreach (var abc in fooArray)
    {
        var temp = abc.Contains("og:description");
        if (temp == true)
        {
            Ftag += "<" + abc.Replace("og:", "");
            var keywords = "<" + abc.Replace("og:description", "keywords");
            keywords = keywords.Replace("For Sale:", "For Sale,");
            keywords = keywords.Replace("For Rent:", "For Rent,");
            keywords = keywords.Replace("For", "property portal, real estate marketplace, buy, sell, rent, For");
            Ftag += keywords;
        }
        var temp2 = abc.Contains("og:title");
        if (temp2 == true)
        {
            Ftag += "<" + abc.Replace("og:", "");

        }
    }
    tempMainTag = Ftag + tempMainTag;
    ViewBag.Meta = tempMainTag;
}

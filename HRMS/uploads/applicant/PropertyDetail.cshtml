    function getSimilarProperty(propertyObj) {
        var propertyType = propertyObj.lookPropertyTypeIdField;
        var marketType = propertyObj.lookPropertyMarketTypeIdField;
        var categoryType = propertyObj.lookPropertyCategoryIdField;
        var thisProperty = propertyObj.propertyGuidField;
        $.ajax({
            type: "GET",
            url: '/Api/PropertyDetail/GetSimilarProperty?propertType=' + propertyType + '&marketTypeId=' + marketType + '&categoryTypeId=' + categoryType + '&propertyGuid=' + thisProperty,
            result: {},
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            success: function (result) {
                if (result.ResultType == 1) {
                    var resultJson = result.Data;
                    if (resultJson != null && resultJson.length > 0) {
                        if (resultJson.length <= 2) {
                            $('#emptySimilarSold').empty();
                            $('#emptySimilarSold').hide();
                        }
                        else {
                            renderSimilarSoldProperties(resultJson);
                            getSoldProperty(resultJson);
                        }
                    }
                    else {
                        $('#emptySimilarSold').empty();
                    }
                }
                else {
                    $('#emptySimilarSold').empty();
                }
            },
            error: function (result) {
                var res = result;
                if (res.status == 400) {
                    location.href = "@Url.Action("Error", "PropertyDetail")";
                }
            }
        });
    }

    function getSoldProperty(resultJson) {
        var marketType = resultJson[0].LookPropertyMarketTypeId;
        var categoryType = resultJson[0].LookPropertyCategoryId;
        $.ajax({
            type: "GET",
            url: '/Api/PropertyDetail/GetSoldProperty?marketTypeId=' + marketType + '&categoryId=' + categoryType,
            result: {},
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            success: function (result) {
                if (result.ResultType == 1) {
                    var resultJson = result.Data;
                    if (resultJson != null) {
                        renderSoldProperties(resultJson);
                    } else {
                        $("#emptySoldArea").hide();
                    }
                }
                else {
                    $("#emptySoldArea").hide();
                }
            },
            error: function (result) {
                var res = result;
                if (res.status == 400) {
                    location.href = "@Url.Action("Error", "PropertyDetail")";
                }
            }
        });
    }


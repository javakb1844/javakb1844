<script type="text/javascript">
    loadPropertyDetail();
    function loadPropertyDetail() {
        $(document).ready(function () {
            var resultJson;
            var getUrl = document.location.href.split("?");
            var url = getUrl[1];

            function getURLParameter(name) {
                return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search) || [null, ''])[1].replace(/\+/g, '%20')) || null;
            }
            if (getURLParameter('ilaanId') != null) {
                url = 'ilaanId_' + getURLParameter('ilaanId');
            }
            $.ajax(
                 {
                     type: "GET",
                     url: '/Api/PropertyDetail/Property?url=' + url,
                     result: {},
                     dataType: "json",
                     contentType: "application/json; charset=utf-8",
                     success: function (result) {
                         if (result.ResultType == 1) {
                             resultJson = result.Data;
                             document.title = "ilaan.com - " + resultJson.headingField;
                             if (resultJson != null) {
                                 if (resultJson.lookPropertyMarketTypeIdField == 1) {
                                     //addMetaTags(resultJson);
                                     loaderSliderImages(resultJson);

                                     initializeSlider();
                                     displaySocialMedia(resultJson);
                                     displayDescription(resultJson);
                                     displayMap(resultJson)
                                     displayAgency(resultJson);
                                     loadFeature(resultJson);
                                     floorPlan(resultJson);
                                     interactiveFloorPlan(resultJson);
                                     playVideo(resultJson);
                                     $("#heading").text(resultJson.headingField);
                                     getSimilarProperty(resultJson);
                                 }
                                 else {
                                     loaderSliderImages(resultJson);

                                     initializeSlider();
                                     displaySocialMedia(resultJson);
                                     displayDescription(resultJson);
                                     displayMap(resultJson)
                                     displayAgency(resultJson);
                                     floorPlan(resultJson);
                                     interactiveFloorPlan(resultJson);
                                     playVideo(resultJson);
                                     $("#heading").text(resultJson.headingField);
                                     getSimilarProperty(resultJson);
                                 }
                             }
                             else {
                                 location.href = "@Url.Action("Error", "PropertyDetail")";
                             }
                         } else {
                             console.log(result.Exception);
                             console.log(result.Message);
                             location.href = "@Url.Action("Error", "PropertyDetail")";
                         }

                     },
                     error: function (result) {
                         var res = result;
                         if (res.status == 400) {
                             console.log(result);
                             location.href = "@Url.Action("Error", "PropertyDetail")";
                         }
                     }
                 });
        });
    }
